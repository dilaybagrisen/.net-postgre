@model IEnumerable<TopEventViewModel>

<h1>En Popüler 5 Etkinlik</h1>

<table class="table">
    <thead>
        <tr>
            <th>Event ID</th>
            <th>Event Title</th>
            <th>Participant Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var eventItem in Model)
        {
        <tr>
            <td>@eventItem.eventid</td>
            <td>@eventItem.eventtitle</td>
            <td>@eventItem.participantcount</td>
        </tr>
        }
    </tbody>
</table>

